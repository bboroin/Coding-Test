const readline = require("readline");

const rl = readline.createInterface({
  input: process.stdin,
  output: process.stdout,
});

let input;

rl.on("line", (line) => {
  input = line;
  rl.close();
});

rl.on("close", () => {
  const [W, R] = input.split(" ").map(Number);
  console.log(Math.floor(W * (1 + R / 30)));
  process.exit();
});
